{
    "name": "ingest-gmaplist",
    "version": "0.0.1",
    "description": "CLI program to ingest Google Maps Lists",
    "author": "Derek Dunagan (https://github.com/rhombal)",
    "license": "MIT",
    "main": "target/npm/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rhombal/uri-query.git"
    },
    "dependencies": {
        "commander": "^2.14.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.18.0",
        "mocha": "^5.0.1",
        "np": "^2.20.1",
        "rimraf": "^2.6.2"
    },
    "scripts": {
        "test": "eslint ./source && mocha ./source/index.test.js",
        "prestart": "npm install --silent",
        "startXXX": "mocha --inspect-brk --watch ./source/index.test.js",
        "start": "node --inspect-brk ./bin/ingest-gmaplist ./source/paris.1.me.txt",
        "build": "npm run --silent build-npm",
        "prebuild-npm": "npm install --silent && npm test --silent",
        "build-npm": "rimraf ./target/npm; babel ./source --out-dir ./target/npm",
        "publish-npm": "git push origin && np",
        "rinse": "rimraf ./target ./.tmp ./tmp ./**/.tmp ./**/tmp ./npm-debug.log* ./**/npm-debug.log*",
        "scrub": "npm run --silent rinse; rimraf ./node_modules"
    }
}
